package com.qdoc.client.model;

/**
 * com.qdoc.client.model.LoginModel
 * 
 * @author xiangyutian <br/>
 *         create at 2014-7-13 下午5:14:53
 */
public class LoginModel extends AbstractBaseModel {

	private static final long serialVersionUID = 1L;
	private String token;//授信凭证
    private int loginStatus;//登录状态
    private int userStatus;//用户权限
    private int doctorCertificationStatus;//认证状态
    
	/**
	 * @return the userStatus
	 */
	public int getUserStatus() {
		return userStatus;
	}

	/**
	 * @param userStatus the userStatus to set
	 */
	public void setUserStatus(int userStatus) {
		this.userStatus = userStatus;
	}

	public int getLoginStatus() {
		return loginStatus;
	}

	public void setLoginStatus(int loginStatus) {
		this.loginStatus = loginStatus;
	}

	/**
     * @return the token
     */
    public String getToken() {
        return token;
    }

    /**
     * @param token the token to set
     */
    public void setToken(String token) {
        this.token = token;
    }

	public int getDoctorCertificationStatus() {
		return doctorCertificationStatus;
	}

	public void setDoctorCertificationStatus(int doctorCertificationStatus) {
		this.doctorCertificationStatus = doctorCertificationStatus;
	}

    
}
