    private String updateTime;//更新时间
    private int userStatus;//用户权限
	private int certificationStatus;//认证状态
	private ArrayList<Long> consultList;

	/**
	 * @return the userStatus
	 */
	public int getUserStatus() {
		return userStatus;
	}

	/**
	 * @param userStatus the userStatus to set
	 */
	public void setUserStatus(int userStatus) {
		this.userStatus = userStatus;
	}

    
    /**
     * @return the consultStatus
     */
    public int getConsultStatus() {
        return consultStatus;
    }

    /**
     * @param consultStatus the consultStatus to set
     */
    public void setConsultStatus(int consultStatus) {
        this.consultStatus = consultStatus;
    }

    /**
     * @return the msgStatus
